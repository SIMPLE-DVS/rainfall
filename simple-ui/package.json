{
  "name": "rainfall-frontend",
  "version": "0.0.1",
  "description": "Frontend for RAINFALL",
  "productName": "RAINFALL Frontend",
  "author": "UniversitÃ  degli Studi di Camerino and Sigma S.p.A",
  "license": "AGPLV3+",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.ts,.vue ./",
    "format": "prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "build": "quasar build",
    "dev": "quasar dev",
    "coverage": "yarn test:e2e:ci && yarn test:component:ci && echo \"Cypress E2E and Component tests' merged coverage. Vitest coverage not included!\"",
    "test": "echo \"See package.json => scripts for available tests.\" && exit 0",
    "test:unit:ui": "vitest --ui",
    "test:unit": "vitest",
    "test:unit:ci": "vitest run",
    "test:unit:coverage": "vitest run --coverage",
    "test:component": "cross-env NODE_ENV=test cypress open --component",
    "test:component:ci": "cross-env NODE_ENV=test cypress run --component",
    "test:e2e": "cross-env NODE_ENV=test start-test \"quasar dev\" http-get://127.0.0.1:7000 \"cypress open --e2e\"",
    "test:e2e:ci": "cross-env NODE_ENV=test start-test \"quasar dev\" http-get://127.0.0.1:7000 \"cypress run --e2e\""
  },
  "dependencies": {
    "@quasar/extras": "^1.0.0",
    "ace-builds": "^1.9.3",
    "axios": "^0.21.1",
    "d3": "7.4.4",
    "pinia": "^2.0.23",
    "quasar": "^2.6.0",
    "vue": "^3.0.0",
    "vue-i18n": "^9.0.0",
    "vue-router": "^4.0.0"
  },
  "devDependencies": {
    "@intlify/vite-plugin-vue-i18n": "^3.3.1",
    "@quasar/app-vite": "^1.0.0",
    "@quasar/quasar-app-extension-testing": "^2.0.5",
    "@quasar/quasar-app-extension-testing-e2e-cypress": "^5.0.0-beta.7",
    "@quasar/quasar-app-extension-testing-unit-vitest": "^0.1.2",
    "@types/d3": "7.4.0",
    "@types/node": "^12.20.21",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "@vitest/ui": "^0.15.0",
    "@vue/test-utils": "^2.0.0",
    "autoprefixer": "^10.4.2",
    "axios-mock-adapter": "^1.21.2",
    "c8": "^7.12.0",
    "cypress": "^10.10.0",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-vue": "^9.0.0",
    "mock-socket": "^9.1.5",
    "prettier": "^2.5.1",
    "typescript": "^4.5.4",
    "vitest": "^0.15.0"
  },
  "engines": {
    "node": "^18 || ^16 || ^14.19",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
